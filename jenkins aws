pipeline{
agent any
stages{
  stage ("create aws stack') {
  
  steps{
  
  echo " create aws stack"
  
  withCredentials([[$class: 'AmazonWebServicesCredentialsBinding', accessKeyVariable: 'AWS_ACCESS_KEY_ID', credentialsId: 'keys', secretKeyVariable: 'AWS_SECRET_ACCESS_KEY']]) {
   
  sh 'aws s3 ls'
  }
  }
  }
  }
